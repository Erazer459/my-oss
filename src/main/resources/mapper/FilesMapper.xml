<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="io.github.franzli347.foss.web.mapper.FilesMapper">

    <resultMap id="BaseResultMap" type="io.github.franzli347.foss.model.entity.Files">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="fileName" column="file_name" jdbcType="VARCHAR"/>
            <result property="fileSize" column="file_size" jdbcType="DOUBLE"/>
            <result property="path" column="path" jdbcType="VARCHAR"/>
            <result property="bid" column="bid" jdbcType="INTEGER"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,file_name,file_size,
        path,bid
    </sql>
</mapper>
